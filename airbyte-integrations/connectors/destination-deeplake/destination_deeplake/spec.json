{
  "documentationUrl": "https://docs.airbyte.io/integrations/destinations/deeplake",
  "supported_destination_sync_modes": ["overwrite", "append"],
  "supportsIncremental": true,
  "supportsDBT": false,
  "supportsNormalization": false,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Deeplake",
    "type": "object",
    "required": ["path"],
    "additionalProperties": false,
    "properties": {
      "path": {
        "title": "Path",
        "type": "string",
        "description": "Destination",
        "order": 1,
        "examples": ["hub://{USERNAME}", "s3://path/to/dataset"]
      },
      "token": {
        "title": "Token",
        "type": "string",
        "description": "token provided from app.activeloop.ai",
        "order": 1,
        "examples": ["{API TOKEN}"]
      }
    }
  }
}
